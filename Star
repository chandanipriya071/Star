
b.  cfc jj. fr BBB dv nii. tv ffv n




public class star {

    public static void main(String[] sd) {
        int rows = 5;
        for (int i = 1; i <= rows; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print("* ");
            }
            System.out.println();
        }
    }
}
